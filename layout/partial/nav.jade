ul.nav.nav-list
    each value, key in theme.menu
        li.nav-list-item
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            - var active = (page.path === "index.html" && value === "/") || (page.type === "tags" && value === "/tags")
            a.nav-list-link(class={active: active} href=value target=tar)
                != key.toUpperCase()
